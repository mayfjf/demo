<%= stylesheet_link_tag "zone" %>
<body>

<div id="header">
<h1>Create New Agency Record</h1>
</div>

<div id="section"><center>
 
 <%= form_for(:agency, :url => {:controller => 'agency', :action => 'create'}) do |f| %>
  
 
     <br>Agency ID:<br><%= f.text_field :id, :value=>"autogenerated",:readonly=>true%> 
     <br>Name:<br><%= f.text_field :name%> 
     <br>Head:<br><%= f.text_field :head%> 
     <br>Priority number:<br><%= f.number_field :prioritynum%> 
     <br>Contact Number:<br><%= f.telephone_field :contactnum%> 
     <br>Responsibilities:<br><%= f.text_area :reponsibilities%> 
    
    
    <br><br><br><%= f.submit "Submit", class: "btn btn-default" %>
  <% end %>


<hr>
    
  </div>
</body>
